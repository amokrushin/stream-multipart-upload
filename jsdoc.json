{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "lib/post-processor/",
      "lib/pre-processor/",
      "lib/transform/"
    ]
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "template": "node_modules/docdash",
    "destination": "./jsdoc/",
    "recurse": true,
    "verbose": true
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": false
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    }
  },
  "docdash": {
    "static": false,
    "sort": true
  }
}